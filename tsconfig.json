{
	"compilerOptions": {
		"sourceMap": true,
		"module":"commonjs",
		"target":"es2017",
		"noImplicitAny":false,
		"noUnusedLocals":true,
		"resolveJsonModule":true,
		"esModuleInterop":true,
		"moduleResolution":"node",
		"outDir":"dist",
		"baseUrl":".",
		"paths": {
			"*": [
				"node_modules/*",
			]
		}
	},
		"include": [
			"./src/**/*"
		]
}
